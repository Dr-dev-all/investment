{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/Landing.js",
    "components/Login.js",
    "app/api/latestnews/route.js",
    "app/layout.js",
    "app/page.js",
    "components/Register.js",
    "components/Footer.js",
    "components/index.js",
    "components/Verifypassword.js",
    "components/Userdash.js",
    "components/About.js",
    "components/AdminDashBoard.js",
    "components/userdashboardHeader.js",
    "components/userdashboardFooter.js",
    "components/Contact.js",
    "components/Header.js",
    "app/(auth)/register/page.js",
    "app/(auth)/login/page.js",
    "../server/models/userModels.js",
    "app/api/register/route.ts",
    "app/(auth)/layout.js",
    "middleware.js"
  ],
  "exclude": ["node_modules"]
}
